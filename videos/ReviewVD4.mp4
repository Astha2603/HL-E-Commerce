    function setVariantId(){
       let filteredArray = global_data.variants.filter(obj => {
          return obj.option1 == product_option[0] && obj.option2 == product_option[1];
       });
      if(filteredArray[0].available){
        if($(".modal-size.active.hide").length == 0){
          variant_id = filteredArray[0].id;
          let price = filteredArray[0].price / 100;
          let cmp =  filteredArray[0].compare_at_price / 100 ;
          let save_parcentage = Math.trunc(((parseInt(cmp) - parseInt(price))/parseInt(cmp))*100);
          act_cmp = cmp;
          // console.log("variant ",filteredArray);
          $(".modal-price").html(`Rs. ${price}`);
          $(".modal-cmp-price").html(`<del>Rs. ${cmp}</del>`);
          $(".modal-prd-discount").html(`${save_parcentage}% OFF`);
        }else {
          product_option[1]=undefined;
          $(".modal-size.active.hide").removeClass("active");
          setPriceAccdingToFirstOption();
        }      
      }
      else{
         // console.log("when user has selected size, after changing color it is unabailable"); 
        product_option[1]=undefined;
        setPriceAccdingToFirstOption();
      }
      
