    function markedOutOfStack(color){
      // console.log("Global data",global_data);
      // console.log("color",color);
      variant_arr=[];
      $(`.modal-size`).removeClass('out-of-stock');
      let filteredArray = global_data.variants.filter(obj => {
            return obj.option1 === color;
      });
      $(hide_show_varient.size).each(function(i,v){
        if(v.includes(color+" / UK")){
          variant_arr.push(v.split("/ ")[1]);
        }
      })
         $(`.modal-size`).removeClass('hide');
      $(filteredArray).each(function(i,v){
        if(variant_arr.includes(v.option2)){
          let $hideSize = $(`.modal-size[data-size='${v.option2}']`);
          $hideSize.addClass('hide');
        }
         // $outOfStockElement.removeClass("active");
         // $outOfStockElement.addClass("out-of-stock");
         // $outOfStockElement.off("click");
      });
      var presentSize = [];
      $(filteredArray).each(function(i,v){
        if(!v.available){
           let $outOfStockElement = $(`.modal-size[data-size='${v.option2}']`);
           $outOfStockElement.removeClass("active");
           $outOfStockElement.addClass("out-of-stock");
           $outOfStockElement.off("click");
        }
         presentSize.push(v.option2);
      });
      if($(".modal-size").length != filteredArray.length){
       $(".modal-size").each(function(i,e){
        if(!presentSize.includes($(e).data("size"))){
           $(e).removeClass("active");
           $(e).addClass("out-of-stock")
         }
       });
     }
     else{
       // console.log("Equal");
     }
    }
